@model IEnumerable<TiendaVideojuegos.Models.Abonados>

@{
    ViewData["Title"] = "Index";
}

<body class="fondo-compra">

    <center> <font color="white"><h1>Abonados</h1></font></center>
<hr />

<p>
    <a class="button7" asp-action="Create">Create New</a>
</p>
<table class="table"  bgcolor="#FFFFFF">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.IdAbonado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DNI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contraseña)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.e_mail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Logueado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdAbonado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DNI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contraseña)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.e_mail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Logueado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAbonado">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAbonado">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAbonado">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>